{
  "name": "ratiokit-screenshottest",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "stop": "lsof -ti:8080 | xargs kill 2>/dev/null || echo 'No process running on port 8080'",
    "stop:all": "pkill -f vite || echo 'No vite processes found'",
    "build": "vite build",
    "build:lib": "tsc",
    "build:css:tailwind": "sass RatioKit/RatioKit.scss:css/RatioKit.css --load-path=RatioKit",
    "build:css:simple": "sass RatioKit/RatioKitSimple.scss:css/RatioKitSimple.css --load-path=RatioKit",
    "rebuild:zip": "bash rebuild_zip.sh",
    "deploy": "bash deploy.sh",
    "deploy:vercel": "vercel --yes --prod --force",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "sass": "^1.97.1",
    "svelte": "^5.46.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vue": "^3.5.26"
  }
}
